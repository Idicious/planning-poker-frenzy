{
	"name": "sveltekit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "svelte-kit sync",
		"test": "vitest",
		"test:coverage": "vitest --coverage",
		"generate-db-types": "supabase gen types typescript --local > src/lib/generated-db-types.ts",
		"db:seed": "ts-node-esm supabase/seed.ts",
		"db:init": "supabase start && npm run db:seed",
		"db:reset": "supabase stop && npm run db:init",
		"test:e2e": "npm run build && playwright test",
		"test:e2e:ci": "npm run build && playwright test --browser chromium",
		"test:e2e:generate": "playwright codegen http://localhost:5173/login"
	},
	"engines": {
		"node": "^18.12.1"
	},
	"devDependencies": {
		"@playwright/test": "1.29.1",
		"@sveltejs/adapter-vercel": "^1.0.0",
		"@sveltejs/kit": "1.0.0",
		"@testing-library/svelte": "^3.2.2",
		"@types/lodash-es": "^4.17.6",
		"@types/node": "^18.11.18",
		"@types/sharp": "^0.31.0",
		"@typescript-eslint/eslint-plugin": "^5.46.0",
		"@typescript-eslint/parser": "^5.46.1",
		"@vitest/coverage-c8": "^0.25.8",
		"autoprefixer": "^10.4.13",
		"cross-fetch": "^3.1.5",
		"dotenv": "^16.0.3",
		"eslint": "^8.29.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"happy-dom": "^8.1.0",
		"msw": "^0.49.2",
		"postcss": "^8.4.18",
		"prettier": "^2.8.1",
		"prettier-plugin-svelte": "^2.9.0",
		"supabase": "^1.27.8",
		"svelte": "^3.55.0",
		"svelte-check": "^2.10.2",
		"svelte-htm": "^1.2.0",
		"svelte-preprocess": "^5.0.0",
		"tailwindcss": "^3.2.3",
		"ts-node": "^10.9.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.3",
		"vitest": "^0.26.2",
		"vitest-mock-extended": "^1.0.9"
	},
	"type": "module",
	"dependencies": {
		"@felte/validator-zod": "^1.0.12",
		"@fortawesome/free-brands-svg-icons": "^6.2.1",
		"@fortawesome/free-solid-svg-icons": "^6.2.1",
		"@prisma/client": "^4.11.0",
		"@supabase/auth-helpers-sveltekit": "^0.8.6",
		"@supabase/supabase-js": "2.4.1",
		"@vercel/analytics": "^0.1.6",
		"felte": "^1.2.6",
		"imagekit": "^4.1.2",
		"inversify": "^6.0.1",
		"lodash-es": "^4.17.21",
		"prisma": "^4.11.0",
		"reflect-metadata": "^0.1.13",
		"sharp": "^0.31.2",
		"svelte-fa": "^3.0.3",
		"web-vitals": "^3.1.0",
		"zod": "^3.20.2"
	}
}
